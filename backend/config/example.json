{
    "auth": {
        "enabled": true,
        "providers": [
            {
                "name": "google",
                "client_id": "{{your_client_id}}",
                "client_secret": "{{your_client_secret}}"
            },
            {
                "name": "azure",
                "client_id": "{{your_client_id}}",
                "client_secret": "{{your_client_secret}}",
                "tenant_id": "{{your_tenant_id}}"
            }
        ],
        "redirect_url": "http://localhost:8000/admin/callback"
    },
    "storage": {
        "File": {
            "config": {
                "path": "rustylol.ron"
            }
        }
    }
}